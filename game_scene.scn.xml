		<string name="script/source"> "extends KinematicBody2D&#000a;&#000a;var dir = 2&#000a;var ani = false&#000a;&#000a;func _ready():&#000a;&#0009;self.set_process(true)&#000a;&#000a;func _process(delta):&#000a;&#000a;&#0009;var sprite = self.get_child(0)&#000a;&#000a;&#0009;var v = 64 * 1.5 # pixels per second. one tile is 16 px&#000a;&#000a;&#0009;var vel = Vector2(0, 0)&#000a;&#0009;if(Input.is_key_pressed(KEY_DOWN)):&#000a;&#0009;&#0009;vel.y += v&#000a;&#0009;&#0009;dir = 2&#000a;&#0009;&#0009;ani = true&#000a;&#0009;if(Input.is_key_pressed(KEY_UP)):&#000a;&#0009;&#0009;vel.y -= v&#000a;&#0009;&#0009;dir = 0&#000a;&#0009;&#0009;ani = true&#000a;&#0009;if(Input.is_key_pressed(KEY_LEFT)):&#000a;&#0009;&#0009;vel.x -= v&#000a;&#0009;&#0009;dir = 0&#000a;&#0009;&#0009;ani = true&#000a;&#0009;if(Input.is_key_pressed(KEY_RIGHT)):&#000a;&#0009;&#0009;vel.x += v&#000a;&#0009;&#0009;dir = 2&#000a;&#0009;&#0009;ani = true&#000a;&#0009;if Input.is_key_pressed(KEY_ESCAPE):&#000a;&#0009;&#0009;get_tree().quit()&#000a;&#000a;&#0009;if vel.x == 0 and vel.y == 0:&#000a;&#0009;&#0009;ani = false&#000a;&#000a;&#0009;move(vel * delta)&#000a;&#000a;&#000a;&#0009;if ani:&#000a;&#0009;&#0009;sprite.move_ani(dir)&#000a;&#0009;else:&#000a;&#0009;&#0009;sprite.idle_ani(dir)&#000a;" </string>

